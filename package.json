		"eslint-plugin-react": "^7.32.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"prettier": "2.8.3",
		"react": "^18.2.0",
		"rimraf": "^4.1.1",
		"typescript": "*"
